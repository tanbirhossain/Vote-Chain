
@{
    ViewData["Title"] = "Create";
}
<script src="~/clipboard.js-master/dist/clipboard.min.js"></script>

<div class="row">
    <div class="col-6">
        <div class="card">
            <div class="card-header bg-transparent">
                Register
            </div>
            <div class="card-body">


                <div class="form-group">
                    <div class="input-group">
                        <div class="floating-label">
                            <label for="exampleInputPassword1">Name</label>
                            <input type="text" class="form-control" id="name" placeholder="">
                        </div>

                    </div>
                </div>

                <div class="form-group">
                    <div class="input-group">
                        <div class="floating-label">
                            <label for="exampleInputEmail1">Phone Number</label>
                            <input type="text" class="form-control" id="phoneNumber" aria-describedby="emailHelp" placeholder="">
                        </div>

                    </div>
                </div>
            </div>
            <div class="card-footer">
                <button type="submit" id="btnSaveInfo" class="btn btn-primary has-spinner">Submit</button>
            </div>
        </div>



    </div>
    <div class="col-6">

        <div class="card">
            <div class="card-header bg-transparent">
                Keys
            </div>


            <div class="card-body">

                <div class="form-group">
                    <label class="col-form-label">Public Key</label>
                    <!-- Trigger -->
                    <button class="copy btn btn-outline-info btn-sm" data-clipboard-target="#publicKey">
                        Copy
                    </button>
                    <!-- Target -->
                    <textarea class="form-control" id="publicKey" rows="4" readonly></textarea>
                </div>
                <hr />
                <div class="form-group">
                    <label class="col-form-label">Private Key</label>
                    <!-- Trigger -->
                    <button class="copy btn btn-outline-info btn-sm" data-clipboard-target="#privateKey">
                        Copy
                    </button>
                    <!-- Target -->
                    <textarea class="form-control" id="privateKey" rows="3" readonly></textarea>
                    <br />
                    <div class="alert alert-danger" role="alert">
                        Don't share your <strong>Private Key</strong> with anyone!
                    </div>
                </div>
                <hr />
                <div class="form-group">
                    <label class="col-form-label ">QR Code</label>
                    <!-- Trigger -->
                    <img id="qrCode" class="img-fluid" src="data:image/jpeg;base64," alt="Responsive image">
                    <div class="alert alert-success" role="alert">
                        Please  <a href="javascript:void(0)" id="qrDownload">Download QR Code</a> <strong></strong> and don't share with anyone!
                    </div>


                </div>


            </div>


        </div>

    </div>
</div>

<script>
    $(document).ready(function () {
        new ClipboardJS('.copy');


    });

    var _name="blank";
    var _base64;
    $("#btnSaveInfo").click(function () {
        var model =
        {
            PhoneNumber: $("#phoneNumber").val(),
            Name: $("#name").val()
        };
        var _url = "/api/profile/GetNewWalletWithQR";
        $.ajax({
            type: "POST",
            url: _url,
            data: model, // serializes the form's elements.

            success: function (data) {
                _name = $("#name").val();
                _base64 = data.qrCodeBase64;
                console.log("update : ", data);
                $('#privateKey').text(data.privateKey);
                $('#publicKey').text(data.publicKey);
                $("#qrCode").attr('src', 'data:image/png;base64,' + data.qrCodeBase64);

            },
            error: function (jqxhr, textstatus, errorthrown) {
                jsonValue = jQuery.parseJSON(jqxhr.responseText);
                alert(jsonValue.Message);
            },
            complete: function () {
            }
        });
    });


    $("#qrDownload").click(function () {
        downloadBase64File("image/png", _base64, _name + "_QR");
    });
    // Parameters:
    // contentType: The content type of your file.
    //              its like application/pdf or application/msword or image/jpeg or
    //              image/png and so on
    // base64Data: Its your actual base64 data
    // fileName: Its the file name of the file which will be downloaded.

    function downloadBase64File(contentType, base64Data, fileName) {
        const linkSource = `data:${contentType};base64,${base64Data}`;
        const downloadLink = document.createElement("a");
        downloadLink.href = linkSource;
        downloadLink.download = fileName;
        downloadLink.click();
    }

</script>